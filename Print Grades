#include<stdio.h>
int main ()
{
  int marks;
  printf ("Enter your marks ");
  scanf ("%d", &marks);
  {
    if (marks > 0 || marks < 100)
      {
	      if (marks > 84 && marks < 101)
	         printf ("Grade A");
        else if (marks < 85 && marks > 69)
           printf ("Grade B");
        else if (marks < 70 && marks > 54)
           printf ("Grade C");
        else if (marks < 55 && marks > 39)
            printf ("Grade D");
        else if (marks > -1 && marks < 40)
             printf ("Grade F");
      }
   else
      {
	printf ("Invalid input");
      }
  }
  return 0;
}
